#import "BH001 Common Styles.hss";

slave
{
   background: bgGrad;
   contentAlignY: middle;

   #new wrapper
   {
      width: 60%;

      #move message
      {
          font: heading1;
          textAlign: center;
      }

      #move tip
      {
          font: stdFont;
          height: 60;
          textAlign: center;
          contentAlignY: middle;
      }

      #move button
      {
          isA: button;
          width: 120;
          height: 40;
          shape: @{5}
          on: @click{
              @request { src: "BH001 Load.xml" }
          }
      }
   }
}
