#0 bgColor;
#5 bgColor2;

master
{
    contentAlignY: 50%;
    background: #3;
    
    #new test
    {
        width: 200;
        height: 50%;
        shape: @{10};
        border: @{ 1 #FFF5 }, @{ 1 #0008 };
